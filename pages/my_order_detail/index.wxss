}
.information,.reiconb{
  border-bottom: 20rpx solid #EEEEEE;
}
.information text{
  display: inline-block;
  margin: 0 50rpx 10rpx 0;
}
.fkItem{
  width: 100%;
  padding: 36rpx 0 10rpx 0;
