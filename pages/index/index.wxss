.refresh-loading {
  width: 50rpx;
  height: 50rpx;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  margin: auto;
}

page {
  background: linear-gradient(#94DECF, #5E93D7);
}

.index {
  position: fixed;
  width: 100%;
  height: calc(100% - 108rpx);
  background: linear-gradient(#94DECF, #5E93D7);
}

.loading {
  padding-bottom: 40rpx;
}

.searchall {
  height: 78rpx;
  border-radius: 39rpx;
}

.top {
  /* 10rpx 54rpx */
  padding: 10rpx 26rpx;
}

.searchall input {
  height: 78rpx;
  line-height: 78rpx;
}

.menu {
  position: relative;
  padding: 160rpx 0;
  overflow: hidden;
  color: white;
}

.earth {
  position: relative;
  /* width: 400rpx; */
  height: 400rpx;
  border: 1rpx solid transparent;
}

.circle {
  width: 400rpx;
  height: 400rpx;
  /* border: 1rpx solid #fff; */
  position: absolute;
  left: 0;
  top: 0;
}

.menucon image {
  width: 125rpx;
  height: 125rpx;
  margin-right: 10rpx;
}

.menuitem {
  margin: -125rpx 0 0 120rpx;
  /* border: 1rpx solid #fff; */
  position: relative;
  height: 670rpx;
}

.menucon {
  margin-bottom: 16rpx;
  position: absolute;
  top: 0;
  left: 0;
}

.menuitem>view:nth-of-type(2) {
  top: 25%;
  left: 220rpx;
  transform: translate(0, -50%);
}

.menuitem>view:nth-of-type(3) {
  top: 50%;
  left: 320rpx;
  transform: translate(0, -50%);
}

.menuitem>view:nth-of-type(4) {
  top: 75%;
  left: 220rpx;
  transform: translate(0, -50%);
}

.menuitem>view:nth-of-type(5) {
  top: unset;
  bottom: 0;
  left: 0;
}

.rubbish {
  width: 244rpx;
  height: 95rpx;
  background: #56E4DA;
  border-radius: 48rpx 0 0 48rpx;
  position: absolute;
  right: 0;
  top: 26rpx;
}

.rubbishpic {
  width: 60rpx;
  height: 60rpx;
  margin: 0 10rpx 0 24rpx;
}

.bg {
  padding: 0 26rpx;
}

.index_putin {
  width: 276rpx;
  height: 84rpx;
  background: rgba(250, 250, 250, 0.6);
  border-radius: 10rpx;
  padding: 28rpx 32rpx;
  box-shadow: 0 0 10rpx #94decf;
}

.wallet {
  width: 88rpx;
  height: 42rpx;
  border: 1rpx solid rgba(102, 102, 102, 1);
  border-radius: 21rpx;
  text-align: center;
  line-height: 42rpx;
}

.login {
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}

.login .miaoshu {
  margin: 400rpx 0 40rpx 0;
  font-size: 28rpx;
}

.button {
  width: 300rpx;
  height: 70rpx;
  border-radius: 60rpx;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  justify-content: center;
}

.button image {
  width: 60rpx;
  height: 60rpx;
  margin-right: 10rpx;
}

.swiper-notice {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin: 20rpx auto 10rpx auto;
  width: 700rpx;
  height: 76rpx;
  background: rgba(255, 255, 255, 0.6);
  box-shadow: 0 0 10rpx #94decf;
  border-radius: 10rpx;
}

.swiper-notice text {
  font-size: 28rpx;
  color: #bfbfbf;
}

.swiper-notice image {
  width: 50rpx;
  height: 50rpx;
}

.swiper-nav {
  height: 80rpx;
  width: 580rpx;
  margin-left: 26rpx;
}

.swiper-nav swiper-item {
  display: flex;
  align-items: center;
}

.swiper-context {
  font-size: 28rpx;
  letter-spacing: 2rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.gotarea {
  margin-right: 50rpx;
  width: 300rpx;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.className {
  animation: twinkling 2s infinite ease-in-out;
}

.animated {
  animation-duration: 2s;
  animation-fill-mode: both;
}

@keyframes twinkling {
  0% {
    opacity: 1;
  }
  50% {
    opacity: .2;
  }
  100% {
    opacity: 1;
  }
}
