<view class='pagetop'>
<import src="../../Components/tab-bar/tab-bar.wxml"/>
<template is="tabBar" data="{{tabBar:bindData.tabBar}}"/>
<view class='bg '>
  <image src='{{l_one.background}}' class='bgpic'></image>
  <navigator url='../setting/setting' class='settting'><view class='iconfont icon-shezhi'></view></navigator>
  <view class="container container1 flex ">
    <view class='head' bindtap='uploadHeadPhoto'>
      <image mode='aspectFill' class='header' src='{{l_one.avatar}}'></image>
    </view>
    <view class='size28 flex_col_1 flex_btw'>
      <view class='badgeitem flex ' wx:if="{{l_one.badge.icon}}"  data-link='{{l_one.badge.control}}' bindtap='link'>
        <image mode='aspectFill' class='badgebg' src='{{l_one.badge.icon}}'></image>
        <view class='badgename'>{{l_one.badge.name}}</view>
      </view>
      <view class='flex align_center'>
        <view  bindtap='name' data-name='{{l_one.nickname}}' class='name'>{{l_one.nickname}} <image mode='aspectFill' class='pen' src='../../images/pen@2x.png'></image> </view>
        <view  data-link='{{l_one.realname.control}}'  bindtap='link'>{{l_one.realname.name}}     </view>
      </view>
      <view class='assetsitem flex_btw'>
        <view data-link='{{l_one.mobile.control}}'  bindtap='link'>{{l_one.mobile.name||"暂未手机号"}} </view>
        <navigator class='assets' url='../assets/index'>
          <view class=' flex_ard align_center'>资产详情 <text class='iconfont icon-jiantou'></text></view>
        </navigator>
      </view>
    </view>
  </view>
</view>
<view class='garbage flex' style='width:{{l_two.length==2?544:652}}rpx'>
  <view wx:for="{{l_two}}" wx:key='index' style='width:{{100/l_two.length}}%' class='center'  bindtap='link' data-link='{{item.control}}'>
    <image src='{{item.icon}}'></image>
    <view class='color4'>{{item.name}}</view>
  </view>
</view>
</view>
<view class='function flex wrap'>
  <view class='funitem' wx:for="{{l_three}}" wx:key='index' bindtap='link' data-link='{{item.control}}'  data-attach='{{item.attach}}'>
    <image src='{{item.icon}}'></image>
    <view class='color4'>{{item.name}}</view>
  </view>
</view>